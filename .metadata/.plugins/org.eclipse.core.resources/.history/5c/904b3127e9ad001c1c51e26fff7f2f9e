package com.simplilearn.list;

import java.util.ArrayList;
import java.util.List;

public class ListExample {

	public static void main(String[] args) {
		

		
		List myList= new ArrayList();// I am creating the ArrayList Object		
		myList.add("A");
		myList.add("B");
		myList.add("C");
		myList.add("D");		
		System.out.println(myList);
		
		
		List myList1= new ArrayList();// I am creating the ArrayList Object		
		myList1.add(1);
		myList1.add(2);
		myList1.add(3);
		myList1.add(4);		
		System.out.println(myList);
		
		
		
		
		
	}
	
}
