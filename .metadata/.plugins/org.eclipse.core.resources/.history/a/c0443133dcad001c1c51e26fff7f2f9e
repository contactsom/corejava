
public class EncapsulationExample {

	
	private int  StuRollNo=10;
    private String StudentName="Joy";
	
    
    public EncapsulationExample(int stuRollNo, String studentName) {
		super();
		StuRollNo = stuRollNo;
		StudentName = studentName;
	}

    //iff <=> if and only if 
    
    
    // Class., Object, constructor 
    
    //it private class has the encaptulation 



}
